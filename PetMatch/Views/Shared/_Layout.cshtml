<!--[if !IE]><!-->
<html lang="en" xmlns="http://www.w3.org/1999/html"><!--<![endif]-->
<head>
    <title>@ViewBag.Title</title>
    <!-- Meta -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="">
    <meta name="author" content="">
    <!-- CSS Global Compulsory-->
    <link rel="stylesheet" href="@Url.Content("~/assets/plugins/bootstrap/css/bootstrap.min.css")">
    <link rel="stylesheet" href="@Url.Content("~/assets/css/style.css")">
    <link rel="stylesheet" href="@Url.Content("~/assets/css/headers/header1.css")">
    <link rel="stylesheet" href="@Url.Content("~/assets/plugins/bootstrap/css/bootstrap-responsive.min.css")">
    <link rel="stylesheet" href="@Url.Content("~/assets/css/style_responsive.css")">
    <link rel="stylesheet" href="@Url.Content("~/assets/css/service-hover.css")">
    <link rel="shortcut icon" href="@Url.Content("~/favicon.ico")">
    <!-- CSS Implementing Plugins -->
    <link rel="stylesheet" href="@Url.Content("~/assets/plugins/font-awesome/css/font-awesome.css")">
    <link rel="stylesheet" href="@Url.Content("~/assets/plugins/flexslider/flexslider.css")" type="text/css" media="screen">
    <link rel="stylesheet" href="@Url.Content("~/assets/plugins/parallax-slider/css/parallax-slider.css")" type="text/css">
    <!-- CSS Theme -->
    <link rel="stylesheet" href="@Url.Content("~/assets/css/themes/default.css")" id="style_color">
    <link rel="stylesheet" href="@Url.Content("~/assets/css/themes/headers/default.css")" id="style_color-header-1">
    @if (IsSectionDefined("AddToHead"))
    {
        @RenderSection("AddToHead", required: false)
    }
</head>
<body>
    <!--=== Top ===-->
    <div class="top">
        <div class="container">
            <div class="top-widget pull-right">
                @Html.Partial("_LoginPartial")
                @*<ul class="loginbar">
                    <li><a href="@Url.Action("Help", "Account")" class="login-btn">Ayuda</a></li>
                    <li class="devider">&nbsp;</li>
                    <li><a href="@Url.Action("Register", "Account")" class="login-btn">Registro</a></li>
                    <li class="devider">&nbsp;</li>
                    <li><a href="@Url.Action("Login", "Account")" class="login-btn">Ingresar</a></li>
                </ul>
                <ul class="social-icons">
                    <li><a href="http://www.facebook.com/thepetmatch" data-original-title="Facebook" class="social_facebook"></a></li>
                    <li><a href="http://www.twitter.com/pet_match" data-original-title="Twitter" class="social_twitter"></a></li>
                    <li><a href="#" data-original-title="Pinterest" class="social_pintrest"></a></li>
                </ul>*@
            </div>
        </div>
    </div><!--/top-->
    <!--=== End Top ===-->
    <!--=== Header ===-->
    <div class="header">
        <div class="container">
            <!-- Logo -->
            <div class="logo">
                <a href="@Url.Action("Index", "Home")">
                    <!--TransForm-->
                    <img id="logo-header" src="@Url.Content("~/assets/img/logo1-default.png")" alt="Logo">
                </a>
            </div><!-- /logo -->
            <!-- Menu -->
            <div class="navbar">
                <div class="navbar-inner">
                    <a class="btn btn-navbar" data-toggle="collapse" data-target=".nav-collapse">
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </a><!-- /nav-collapse -->
                    <div class="nav-collapse collapse">
                        <ul class="nav top-2">
                            <li class="active">
                            <li><a href="@Url.Action("Index", "Home")">Home</a></li>
                            </a>
                            <b class="caret-out"></b>
                            </li>
                            <li>
                            <li><a href="@Url.Action("About", "Home")">Acerca</a></li>
                            </a>

                            <b class="caret-out"></b>
                            </li>
                            <li>
                                <a href="@Url.Action("Index", "Search")" class="dropdown-toggle" data-toggle="dropdown">
                                    Búsqueda
                                    <b class="caret"></b>
                                </a>

                                <b class="caret-out"></b>
                            </li>
                            <li>
                            <li><a href="@Url.Action("Index", "Membership")">Planes</a></li>
                            </a>

                            <b class="caret-out"></b>
                            </li>
                            <li>
                            <li><a href="blog_left_sidebar.html">Blog</a></li>
                            </a>
                            </li>
                            <li>
                            <li><a href="@Url.Action("Contact", "Home")">Contacto</a></li>

                            </a>
                            <b class="caret-out"></b>
                            </li>

                        </ul>
                    </div><!-- /nav-collapse -->
                </div><!-- /navbar-inner -->
            </div><!-- /navbar -->
        </div><!-- /container -->
    </div><!--/header -->
    <!--=== End Header ===-->
    @RenderBody()
    <!--=== Footer ===-->
    <!--=== End Footer ===-->
    <!--=== Copyright ===-->
    <div class="copyright">
        <div class="container">
            <div class="row-fluid">
                <div class="span8">
                    <p>2014 &copy; Pet Match. Todos los Derechos Reservados. <a href="@Url.Action("Privacy", "Home")">Política de privacidad</a> | <a href="@Url.Action("Terms", "Home")">Términos y condiciones</a></p>
                </div>
                <div class="span4"></div>
            </div><!--/row-fluid-->
        </div><!--/container-->
    </div><!--/copyright-->
    <!--=== End Copyright ===-->
    <!-- JS Global Compulsory -->
    <script type="text/javascript" src="@Url.Content("~/assets/js/jquery-1.8.2.min.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/assets/js/modernizr.custom.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/assets/plugins/bootstrap/js/bootstrap.min.js")"></script>
    <!-- JS Implementing Plugins -->
    <script type="text/javascript" src="@Url.Content("~/assets/plugins/flexslider/jquery.flexslider-min.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/assets/plugins/parallax-slider/js/modernizr.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/assets/plugins/parallax-slider/js/jquery.cslider.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/assets/plugins/back-to-top.js")"></script>
    <!-- JS Page Level -->
    <script type="text/javascript" src="@Url.Content("~/assets/js/services-hover.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/assets/js/app.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/assets/js/pages/index.js")"></script>
    <script type="text/javascript">
        jQuery(document).ready(function () {
            App.init();
            //App.initSliders();
            //Index.initParallaxSlider();
        });
    </script>
    <!--[if lt IE 9]>
        <script src="@Url.Content("~/assets/js/respond.js")"></script>
    <![endif]-->
    @RenderSection("scripts", required: false)
</body>
</html>	